<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/rounded_corner">

    <TextView
        android:id="@+id/reg_max"
        android:layout_width="63dp"
        android:layout_height="86dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:background="@color/drk_green"
        android:fontFamily="sans-serif"
        android:gravity="center"
        android:text="2"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="#FFFFFF"
        android:textSize="64sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/hr_parking"
        android:layout_width="138dp"
        android:layout_height="84dp"
        android:layout_marginEnd="8dp"
        android:gravity="center"
        android:text="@string/hr_parking"
        android:textColor="@color/drk_green"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/reg_max"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/reg_max"
        app:layout_constraintTop_toTopOf="@+id/reg_max" />

    <View
        android:id="@+id/view2"
        android:layout_width="0dp"
        android:layout_height="10dp"
        android:background="@color/drk_green"
        app:layout_constraintBottom_toBottomOf="@+id/reg_max"
        app:layout_constraintEnd_toEndOf="@+id/hr_parking"
        app:layout_constraintStart_toEndOf="@+id/reg_max" />

    <TextView
        android:id="@+id/reg_start"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="8dp"
        android:text="8AM"
        android:textColor="@color/drk_green"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toStartOf="@+id/dash"
        app:layout_constraintTop_toTopOf="@+id/dash" />

    <TextView
        android:id="@+id/reg_end"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="12dp"
        android:text="6PM"
        android:textColor="@color/drk_green"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/dash"
        app:layout_constraintTop_toTopOf="@+id/dash" />

    <TextView
        android:id="@+id/dash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginEnd="8dp"
        android:text="-"
        android:textColor="@color/drk_green"
        android:textSize="32sp"
        app:layout_constraintEnd_toStartOf="@+id/reg_end"
        app:layout_constraintStart_toEndOf="@+id/reg_max"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <TextView
        android:id="@+id/reg_start_day"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginTop="12dp"
        android:text="MON"
        android:textColor="@color/drk_green"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/reg_start"
        app:layout_constraintTop_toTopOf="@+id/dash2" />

    <TextView
        android:id="@+id/dash2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:text="-"
        android:textColor="@color/drk_green"
        android:textSize="32sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/dash"
        app:layout_constraintTop_toBottomOf="@+id/dash" />

    <TextView
        android:id="@+id/reg_end_day"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="FRI"
        android:textColor="@color/drk_green"
        android:textSize="15sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/reg_end"
        app:layout_constraintTop_toTopOf="@+id/dash2" />

</androidx.constraintlayout.widget.ConstraintLayout>